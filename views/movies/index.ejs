<% Search.forEach(function(movie) { %>
	<a class="well-link" href="search/<%= movie.imdbID%>">
		<div class="well">
			<%= movie.Title; %>
		</div>
	</a>
<% }); %>

<hr>

<div class="row">
	<div class="col-xs-10 col-sm-6 col-md-6 col-lg-6 col-xs-offset-1 col-sm-offset-3 col-md-offset-3 col-lg-offset-3 btn-center">
		<h2>Sed finibus leo ac arcu vulputate, nec euismod.</h2>
		<p>
			<form action="/search" method="GET" role="search">
				<input class="form-control input-lg" type="text" placeholder="Different Movie Title Keyword" name="q" autocomplete="off" required>
				<button class="btn btn-lg btn-success btn-input" role="button" type="submit">Search</button>
			</form>
		</p>
	</div>
</div>